@model KuaförRandevu00.Models.İletisim

@{
    Layout = "_LayoutAdmin";
}

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Detay</title>
</head>
<body>

    <div>
        <h4>İletisim</h4>
        <hr />
        <table class="table table-bordered table-striped">


            <thead>
                <tr>
                    <th scope="col">@Html.DisplayNameFor(model => model.LogoYazısı)</th>
                    <th scope="col">@Html.DisplayFor(modelItem => Model.LogoYazısı)</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.Baslik)</th>
                    <td>@Html.DisplayFor(modelItem => Model.Baslik)</td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.Aciklama)</th>
                    <td>@Html.DisplayFor(modelItem => Model.Aciklama)</td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.Baslik1)</th>
                    <td colspan="2">@Html.DisplayFor(modelItem => Model.Baslik1)</td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.Aciklama1)</th>
                    <td colspan="2">@Html.DisplayFor(modelItem => Model.Aciklama1)</td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.Adres)</th>
                    <td colspan="2">@Html.DisplayFor(modelItem => Model.Adres)</td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.TelNo)</th>
                    <td colspan="2">@Html.DisplayFor(modelItem => Model.TelNo)</td>
                </tr>
                <tr>
                    <th scope="row">@Html.DisplayNameFor(model => model.EPosta)</th>
                    <td colspan="2">@Html.DisplayFor(modelItem => Model.EPosta)</td>
                </tr>


            </tbody>

        </table>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.Id">Düzenle</a> |
        <a asp-action="Index">Geri Dön</a>
    </div>
</body>
</html>
